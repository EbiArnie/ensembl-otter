cpg=1
fgenesh=1
est2genome_fish=1
est2genome_mouse=1
vertrna=1

[human.use_filters]
est2genome_fish=0
refseq_zebrafish=0

[pig.use_filters]
est2genome_fish=0
refseq_human=0

[dog.use_filters]
est2genome_fish=0
refseq_human=0

[cat.use_filters]
est2genome_fish=0
refseq_human=0

[mouse.use_filters]
est2genome_fish=0
refseq_human=0
refseq_mouse=1

[rat.use_filters]
est2genome_fish=0
refseq_human=0
refseq_rat=1

[zebrafish.use_filters]
est2genome_human=0
est2genome_mouse=0
refseq_human=0
refseq_zebrafish=1
est2genome_clusters=1

[chicken.use_filters]
refseq_chicken=0
refseq_human=0

[default.filter.RepeatMask]
module=Bio::EnsEMBL::Ace::Filter::RepeatMasker

[default.filter.trf]
module=Bio::EnsEMBL::Ace::Filter::TRF

[default.filter.Est2Genome_human]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:
method_tag=EST_Human
homol_tag=EST_homol

[default.filter.Est2Genome_fish]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:
method_tag=EST_Fish
homol_tag=EST_homol

[default.filter.Est2Genome_mouse]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:
method_tag=EST_Mouse
homol_tag=EST_homol

[default.filter.Est2Genome_other]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:
method_tag=EST
homol_tag=EST_homol

[default.filter.est2genome_clusters]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
method_tag=EST_Clusters
homol_tag=DNA_homol

[default.filter.vertrna]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:
method_tag=vertebrate_mRNA
homol_tag=vertebrate_mRNA_homol

[default.filter.genscan]
module=Bio::EnsEMBL::Ace::Filter::Gene::Predicted

[default.filter.Fgenesh]
module=Bio::EnsEMBL::Ace::Filter::Gene::Predicted

[default.filter.CpG]
module=Bio::EnsEMBL::Ace::Filter::SimpleFeature::CpG
method_tag=Predicted_CpG_island

[default.filter.Halfwise]
module=Bio::EnsEMBL::Ace::Filter::Gene::Halfwise
url_string=http:\/\/www.sanger.ac.uk\/cgi-bin\/Pfam\/getacc?%s

# This url requires a LocusLink ID
# http://www.ncbi.nlm.nih.gov/LocusLink/help.html#LinkingtoaSpecificRecord
# url_string=http\:\/\/www.ncbi.nlm.nih.gov\/LocusLink\/LocRpt.cgi?l=?%s
########################################
# This url does a search as described by
# http://www.ncbi.nlm.nih.gov/LocusLink/help.html#CreatingaLink

[default.filter.refseq_human]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
url_string=http:\/\/www.ncbi.nlm.nih.gov\/LocusLink\/list.cgi?ORG\=Hs\&Q\=%s
method_tag=REFSEQ
homol_tag=DNA_homol

[default.filter.refseq_zebrafish]
