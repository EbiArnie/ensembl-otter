#!/usr/local/bin/perl -w

### test_vega_xml_parser

use strict;

use Bio::Vega::Transform::Otter;

{
    my $parser = Bio::Vega::Transform::Otter->new;
    $parser->parsefile(shift);
    weaken($parser);
}

warn "Parser now out of scope\n";


__END__

